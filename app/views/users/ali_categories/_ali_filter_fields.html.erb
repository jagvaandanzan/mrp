<% unless f.object.mn_change %>
<tr class="nested-fields">
  <td>
    <%= field_errors f.object, :name %>
    <%= f.text_field :name, class: 'form-control field', placeholder: t('activerecord.attributes.ali_category.name') %>
  </td>
  <td>
    <%= field_errors f.object, :name_mn %>
    <%= f.text_field :name_mn, class: 'form-control field', placeholder: t('activerecord.attributes.ali_category.name_mn') %>
  </td>
  <td class="remove">
    <%= link_to_remove_association f, data: {confirm: t('are_you_sure')} do %>
      <%= nested_remove_btn %>
    <% end %>
  </td>
</tr>
<% end %>